</div>
</div>

<!-- End Content -->
</div>
  <script>
    function menuCall(){
       	var element=document.getElementById("home");
		document.getElementById("menu").style.display="block";
        element.style.display = "none";
    }
    function homeCall(){
      var element=document.getElementById("home");
		document.getElementById("menu").style.display="none";
        element.style.display = "block";
    
    }
